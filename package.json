{
  "name": "mcp-maker",
  "version": "0.1.3",
  "description": "CLI utility to create TypeScript MCP servers in modular structure.",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "cli",
    "typescript",
    "server",
    "generator"
  ],
  "homepage": "https://github.com/yadav-prakhar/mcp-maker#readme",
  "bugs": {
    "url": "https://github.com/yadav-prakhar/mcp-maker/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yadav-prakhar/mcp-maker.git"
  },
  "license": "Apache-2.0",
  "author": "Prakhar Yadav <codewithp.dev@gmail.com>",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "mcp-maker": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && cp -r src/templates dist/ && chmod +x dist/index.js",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/handlebars": "^4.1.0",
    "@types/node": "^20.11.24",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.19.0"
  }
}
